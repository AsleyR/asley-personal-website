generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model projects {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  description String
  image       String
  repoUrl     String
  role        String
  tags        String[]
  title       String
  url         String?
}
